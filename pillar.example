avahi_config:
  host-name: localhost
  domain-name: local
  use-ipv4: 'yes'
  use-ipv6: 'yes'
  ratelimit-interval-usec: 1000000
  ratelimit-burst: 1000
  enable-wide-area: 'yes'
  rlimit-core: 0
  rlimit-data: 4194304
  rlimit-fsize: 0
  rlimit-nofile: 768
  rlimit-stack: 4194304
  rlimit-nproc: 3
  rlimits_disable_all: True